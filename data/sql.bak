/**订单信息表**/
CREATE TABLE f_order(
  oid INT PRIMARY KEY AUTO_INCREMENT,
  rcvName VARCHAR(16),
  rcvAddr VARCHAR(64),
  rvcPhone VARCHAR(24),
  price FLOAT(8,2),     #999999.99
  payment INT,          #1表货到付款 2表支付宝支付 3京东支付  4在线支付
  status INT,           #1表待付款  2表配货中  3表运输中  4表派货中   5表订单完成  6表废单
  orderTime BIGINT,
  userId INT
);
